<tr class="subjects">
  <td><%= user_item_counter + 1%></td>
  <td><%= user_item.user_name %></td>
  <td><%= user_item.user_email %></td>
  <td>
    <% if current_user != user_item.user %>
      <%= link_to user_item, method: :delete, remote: true, data: {confirm: t("delete_confirm")},
      class: "text-danger", "data-mdb-toggle":"tooltip", title: t("delete") do %>
        <i class="fa fa-trash-alt me-3"></i>
      <% end %>
    <% end %>
  </td>
</tr>
